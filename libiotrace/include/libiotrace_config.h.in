#ifndef LIBIOTRACE_CONFIG_H
#define LIBIOTRACE_CONFIG_H

// All the options configured by CMake
#define LIBIOTRACE_VERSION_MAJOR @LIBIOTRACE_VERSION_MAJOR@
#define LIBIOTRACE_VERSION_MINOR @LIBIOTRACE_VERSION_MINOR@

/* Define to 1 if system has <pthread.h> header file. */
#cmakedefine HAVE_PTHREAD_H 1

/* Define to 1 if system has <stdbool.h> header file. */
#cmakedefine HAVE_STDBOOL_H 1

/* Define to 1 if system has <stdint.h> header file. */
#cmakedefine HAVE_STDINT_H 1

/* Define to 1 if system has <stdlib.h> header file. */
#cmakedefine HAVE_STDLIB_H 1

/* Define to 1 if system has <sys/fcntl.h> header file. */
#cmakedefine HAVE_SYS_FCNTL_H 1

/* Define to 1 if system has <sys/types.h> header file. */
#cmakedefine HAVE_SYS_TYPES_H 1

/* Define to 1 if system has <sys/stat.h> header file. */
#cmakedefine HAVE_SYS_STAT_H 1

/* Define to 1 if system has <sys/syscall.h> header file. */
#cmakedefine HAVE_SYS_SYSCALL_H 1

/* Define to 1 if system has <unistd.h> header file. */
#cmakedefine HAVE_UNISTD_H 1

// These are checked by CheckCompilerAttributes.cmake
/* Define to 1 if compiler supports attribute nonnull. */
#cmakedefine HAVE_ATTRIBUTE_NONNULL 1

/* Define to 1 if compiler supports attribute unused. */
#cmakedefine HAVE_ATTRIBUTE_UNUSED 1

/* Define to 1 if compiler supports attribute constructor. */
#cmakedefine HAVE_ATTRIBUTE_CONSTRUCTOR 1

/*****************************************************************************/
/*  Following is not dependent on cmake; rather evaluates from the previous  */
/*****************************************************************************/


#ifdef HAVE_ATTRIBUTE_NONNULL
#  define ATTRIBUTE_NONNULL(x...)  __attribute__((nonnull(x)))
#else
#  define ATTRIBUTE_NONNULL(x...)
#endif

#ifdef HAVE_ATTRIBUTE_UNUSED
#  define ATTRIBUTE_UNUSED         __attribute__((unused))
#else
#  define ATTRIBUTE_UNUSED
#endif

#ifdef HAVE_ATTRIBUTE_CONSTRUCTOR
#  define ATTRIBUTE_CONSTRUCTOR    __attribute__((constructor))
#else
#  define ATTRIBUTE_CONSTRUCTOR
#endif

#undef BEGIN_C_DECLS
#undef END_C_DECLS
#if defined(c_plusplus) || defined(__cplusplus)
#  define BEGIN_C_DECLS          extern "C" {
#  define END_C_DECLS            }
#else
#  define BEGIN_C_DECLS          /* empty */
#  define END_C_DECLS            /* empty */
#endif

#endif /* LIBIOTRACE_CONFIG_H */
