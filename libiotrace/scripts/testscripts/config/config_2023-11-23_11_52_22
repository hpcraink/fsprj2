base_path="/home/es/es_es/es_pkoester/test_libiotrace_script"

tests="./tests"
test_config="./config"

# source influxdb config file
influxdb_config="$(dirname "$0")/${config_dir}/influxdb"
if [ -f ${influxdb_config} ]; then
    source ${influxdb_config}
else
    echo "influxdb config file ${influxdb_config} does not exist"
    exit
fi

test_influx_dir="${base_path}/${INFLUXDB_NAME}"
test_dir="${base_path}/scripts"

